<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sale_order_quality_check_wizards_form" model="ir.ui.view">
        <field name="name">Sale Order Quality Check Wizards Form</field>
        <field name="model">sale.order.quality.check.wizards</field>
        <field name="arch" type="xml">
            <form>
                <h3>Quality Check Wizards</h3>
                <footer>
                    <button name="quality_check" type="object" string="Check"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>